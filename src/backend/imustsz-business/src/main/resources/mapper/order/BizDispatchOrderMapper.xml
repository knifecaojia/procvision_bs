<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imustsz.order.mapper.BizDispatchOrderMapper">
    
    <resultMap type="BizDispatchOrder" id="BizDispatchOrderResult">
        <result property="id"    column="id"    />
        <result property="processCode"    column="process_code"    />
        <result property="processName"    column="process_name"    />
        <result property="dispatchQuantity"    column="dispatch_quantity"    />
        <result property="startTime"    column="start_time"    />
        <result property="endTime"    column="end_time"    />
        <result property="workerCode"    column="worker_code"    />
        <result property="workerName"    column="worker_name"    />
        <result property="orderId"    column="order_id"    />
        <result property="createdTime"    column="created_time"    />
        <result property="updatedTime"    column="updated_time"    />
        <result property="createdBy"    column="created_by"    />
        <result property="updatedBy"    column="updated_by"    />
        <result property="remarks"    column="remarks"    />
    </resultMap>

    <sql id="selectBizDispatchOrderVo">
        select id, process_code, process_name, dispatch_quantity, start_time, end_time, worker_code, worker_name, order_id, created_time, updated_time, created_by, updated_by, remarks from biz_dispatch_order
    </sql>

    <select id="selectBizDispatchOrderList" parameterType="BizDispatchOrder" resultMap="BizDispatchOrderResult">
        <include refid="selectBizDispatchOrderVo"/>
        <where>  
            <if test="processCode != null  and processCode != ''"> and process_code = #{processCode}</if>
            <if test="processName != null  and processName != ''"> and process_name like concat('%', #{processName}, '%')</if>
            <if test="dispatchQuantity != null "> and dispatch_quantity = #{dispatchQuantity}</if>
            <if test="startTime != null "> and start_time = #{startTime}</if>
            <if test="endTime != null "> and end_time = #{endTime}</if>
            <if test="workerCode != null  and workerCode != ''"> and worker_code = #{workerCode}</if>
            <if test="workerName != null  and workerName != ''"> and worker_name like concat('%', #{workerName}, '%')</if>
            <if test="orderId != null "> and order_id = #{orderId}</if>
            <if test="createdTime != null "> and created_time = #{createdTime}</if>
            <if test="updatedTime != null "> and updated_time = #{updatedTime}</if>
            <if test="createdBy != null  and createdBy != ''"> and created_by = #{createdBy}</if>
            <if test="updatedBy != null  and updatedBy != ''"> and updated_by = #{updatedBy}</if>
            <if test="remarks != null  and remarks != ''"> and remarks = #{remarks}</if>
        </where>
    </select>
    
    <select id="selectBizDispatchOrderById" parameterType="Long" resultMap="BizDispatchOrderResult">
        <include refid="selectBizDispatchOrderVo"/>
        where id = #{id}
    </select>

    <insert id="insertBizDispatchOrder" parameterType="BizDispatchOrder" useGeneratedKeys="true" keyProperty="id">
        insert into biz_dispatch_order
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="processCode != null">process_code,</if>
            <if test="processName != null">process_name,</if>
            <if test="dispatchQuantity != null">dispatch_quantity,</if>
            <if test="startTime != null">start_time,</if>
            <if test="endTime != null">end_time,</if>
            <if test="workerCode != null">worker_code,</if>
            <if test="workerName != null">worker_name,</if>
            <if test="orderId != null">order_id,</if>
            <if test="createdTime != null">created_time,</if>
            <if test="updatedTime != null">updated_time,</if>
            <if test="createdBy != null">created_by,</if>
            <if test="updatedBy != null">updated_by,</if>
            <if test="remarks != null">remarks,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="processCode != null">#{processCode},</if>
            <if test="processName != null">#{processName},</if>
            <if test="dispatchQuantity != null">#{dispatchQuantity},</if>
            <if test="startTime != null">#{startTime},</if>
            <if test="endTime != null">#{endTime},</if>
            <if test="workerCode != null">#{workerCode},</if>
            <if test="workerName != null">#{workerName},</if>
            <if test="orderId != null">#{orderId},</if>
            <if test="createdTime != null">#{createdTime},</if>
            <if test="updatedTime != null">#{updatedTime},</if>
            <if test="createdBy != null">#{createdBy},</if>
            <if test="updatedBy != null">#{updatedBy},</if>
            <if test="remarks != null">#{remarks},</if>
         </trim>
    </insert>

    <update id="updateBizDispatchOrder" parameterType="BizDispatchOrder">
        update biz_dispatch_order
        <trim prefix="SET" suffixOverrides=",">
            <if test="processCode != null">process_code = #{processCode},</if>
            <if test="processName != null">process_name = #{processName},</if>
            <if test="dispatchQuantity != null">dispatch_quantity = #{dispatchQuantity},</if>
            <if test="startTime != null">start_time = #{startTime},</if>
            <if test="endTime != null">end_time = #{endTime},</if>
            <if test="workerCode != null">worker_code = #{workerCode},</if>
            <if test="workerName != null">worker_name = #{workerName},</if>
            <if test="orderId != null">order_id = #{orderId},</if>
            <if test="createdTime != null">created_time = #{createdTime},</if>
            <if test="updatedTime != null">updated_time = #{updatedTime},</if>
            <if test="createdBy != null">created_by = #{createdBy},</if>
            <if test="updatedBy != null">updated_by = #{updatedBy},</if>
            <if test="remarks != null">remarks = #{remarks},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBizDispatchOrderById" parameterType="Long">
        delete from biz_dispatch_order where id = #{id}
    </delete>

    <delete id="deleteBizDispatchOrderByIds" parameterType="String">
        delete from biz_dispatch_order where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>