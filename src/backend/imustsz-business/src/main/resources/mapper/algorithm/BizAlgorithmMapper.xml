<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imustsz.algorithm.mapper.BizAlgorithmMapper">
    
    <resultMap type="BizAlgorithm" id="BizAlgorithmResult">
        <result property="id"    column="id"    />
        <result property="code"    column="code"    />
        <result property="name"    column="name"    />
        <result property="size"    column="size"    />
        <result property="version"    column="version"    />
        <result property="desc"    column="desc"    />
        <result property="objectName"    column="object_name"    />
    </resultMap>

    <sql id="selectBizAlgorithmVo">
        select id, name, version, size, `desc`, object_name from biz_algorithm
    </sql>

    <select id="selectBizAlgorithmList" parameterType="BizAlgorithm" resultMap="BizAlgorithmResult">
        <include refid="selectBizAlgorithmVo"/>
        <where>  
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="code != null  and code != ''"> and name like concat('%', #{code}, '%')</if>
            <if test="version != null  and version != ''"> and version = #{version}</if>
            <if test="size != null  and version != ''"> and size = #{size}</if>
            <if test="desc != null  and desc != ''"> and `desc` = #{desc}</if>
            <if test="objectName != null  and objectName != ''"> and object_name like concat('%', #{objectName}, '%')</if>
            <if test="createTime != null and createTime != ''"> and create_time = #{createTime}</if>
            <if test="createTime != null and createTime != ''"> and update_time = #{updateTime}</if>
            <if test="createBy != null and createBy != ''"> and create_by = #{createBy}</if>
            <if test="updateBy != null and updateBy != ''"> and update_by = #{updateBy}</if>
            <if test="remark != null and remark != ''"> and remark = #{remark}</if>
        </where>
    </select>
    
    <select id="selectBizAlgorithmById" parameterType="Long" resultMap="BizAlgorithmResult">
        <include refid="selectBizAlgorithmVo"/>
        where id = #{id}
    </select>

    <insert id="insertBizAlgorithm" parameterType="BizAlgorithm" useGeneratedKeys="true" keyProperty="id">
        insert into biz_algorithm
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="code != null">code,</if>
            <if test="name != null">name,</if>
            <if test="version != null">version,</if>
            <if test="size != null">size,</if>
            <if test="desc != null">`desc`,</if>
            <if test="objectName != null">object_name,</if>
            <if test="createTime != null and createTime != ''">create_time,</if>
            <if test="createTime != null and createTime != ''">update_time,</if>
            <if test="createBy != null and createBy != ''">create_by,</if>
            <if test="updateBy != null and updateBy != ''">update_by,</if>
            <if test="remark != null and remark != ''">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="code != null">#{code},</if>
            <if test="name != null">#{name},</if>
            <if test="version != null">#{version},</if>
            <if test="size != null">#{size},</if>
            <if test="desc != null">#{desc},</if>
            <if test="objectName != null">#{objectName},</if>
            <if test="createTime != null and createTime != ''">#{createTime},</if>
            <if test="createTime != null and createTime != ''">#{updateTime},</if>
            <if test="createBy != null and createBy != ''">#{createBy},</if>
            <if test="updateBy != null and updateBy != ''">#{updateBy},</if>
            <if test="remark != null and remark != ''">#{remark},</if>
         </trim>
    </insert>

    <update id="updateBizAlgorithm" parameterType="BizAlgorithm">
        update biz_algorithm
        <trim prefix="SET" suffixOverrides=",">
            <if test="code != null">name = #{code},</if>
            <if test="name != null">name = #{name},</if>
            <if test="version != null">version = #{version},</if>
            <if test="size != null">size = #{size},</if>
            <if test="desc != null">`desc` = #{desc},</if>
            <if test="objectName != null">object_name = #{objectName},</if>
            <if test="createTime != null and createTime != ''">create_time = #{createTime},</if>
            <if test="createTime != null and createTime != ''">update_time = #{updateTime},</if>
            <if test="createBy != null and createBy != ''">create_by = #{createBy},</if>
            <if test="updateBy != null and updateBy != ''">update_by = #{updateBy},</if>
            <if test="remark != null and remark != ''">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBizAlgorithmById" parameterType="Long">
        delete from biz_algorithm where id = #{id}
    </delete>

    <delete id="deleteBizAlgorithmByIds" parameterType="String">
        delete from biz_algorithm where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <select id="getAlgorithmVOList" resultMap="BizAlgorithmResult">
        select id, name, size, version, object_name, `desc` from biz_algorithm
    </select>
</mapper>